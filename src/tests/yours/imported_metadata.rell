function test_imported_metadata_retains_expected_attributes() {
  val alice = create_test_account(rell.test.keypairs.alice);
  
  val collection = "TestCollection";
  val token_id = 0;

  rell.test
    .tx()
    .op(
      util.mock_apply_transfer(
        alice.account.id,
        0,
        1,
        yours.token_metadata(
          name = "test",
          attributes = set([
            yours.token_attribute(trait_type = "IntegerKey", integer(1).to_gtv()),
            yours.token_attribute(trait_type = "TextKey", "TextValue".to_gtv())
          ]),
          yours = yours.yours_metadata(
            project = "TestProject",
            collection = collection,
            modules = ["TestModule"]
          ),
          description = "TestDescription",
          image = "https://assets.anichess.com/orb/1.jpg",
          animation_url = "https://assets.anichess.com/orb/1.mp4"
        )
      )
    )
    .run();

  val metadata = yours.metadata(collection, token_id);
  log("Metadata: ", metadata);
}