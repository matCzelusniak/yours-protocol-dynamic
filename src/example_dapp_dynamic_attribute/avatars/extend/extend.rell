
// // one of the options for strict checking attributes and can be decided by user of lib
@extend(originals_light.before_register_asset)
function (
  name,
  symbol: text,
  decimals: integer,
  blockchain_rid: byte_array,
  icon_url: text,
  type: text = ft_assets.ASSET_TYPE_FT4,
  attributes: list<(
    attribute_name: text,
    type: originals_light.attribute_type,
    attribute_value: byte_array
  )>?,
  interfaces: list<byte_array>?,
) {
  print("xyz before_register_asset");
  require(
    attributes != null,
    "Attributes are required"
  );

  if(name == "avatar"){
    originals_light.check_required_attributes(
      attributes!!, 
      list([("fishes_caught", originals_light.attribute_type.integer)])
    );
  }
  else if(name == "fishing_rod"){
    originals_light.check_required_attributes(
      attributes!!, 
      list([("durability", originals_light.attribute_type.big_integer)])
    );
  }
}