struct pudgy_rod {
  id: byte_array;
  name: name;
  durability: big_integer;
}

query get_rods(): list<pudgy_rod> {
  return originals_light.attribute @* {
    .asset.name == "fishing_rod",
    .name == "durability"
  } (
    pudgy_rod(
      id = $.asset.id,
      name = $.asset.name,
      durability = big_integer.from_bytes($.value)
    )
  );
}