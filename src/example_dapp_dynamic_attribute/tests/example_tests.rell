function test_mint_avatar() {
    val account_id = x"01";
    avatars.register_account(
        account_id,
        "test_account"
    ).run();

    avatars.register_and_mint_avatar(
        "avatar",
        "AVT",
        0,
        chain_context.blockchain_rid,
        "https://example.com/avatar.png",
        ft_assets.ASSET_TYPE_FT4,
        [
            (
              attribute_name  = "fishes_caught",
              type = yours.attribute_type.integer,
              attribute_value = big_integer(0).to_bytes()
            )
        ],
        [
            account_id
        ],
        1
    ).run();
}

